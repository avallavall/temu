{
  "name": "temu",
  "version": "0.1.0",
  "description": "Terminal Engine for Multi-agent Unification - A Claude Code clone powered by Ollama",
  "type": "module",
  "main": "dist/lib.js",
  "exports": {
    ".": "./dist/lib.js",
    "./cli": "./dist/index.js"
  },
  "bin": {
    "temu": "dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts src/lib.ts --format esm --dts --clean",
    "dev": "tsx src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "coding",
    "agent",
    "ollama",
    "cli",
    "terminal"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "diff": "^7.0.0",
    "execa": "^9.5.1",
    "glob": "^11.0.0",
    "gray-matter": "^4.0.3",
    "marked": "^14.1.0",
    "marked-terminal": "^7.2.0",
    "openai": "^4.73.0",
    "ora": "^8.1.0",
    "readline": "^1.3.0",
    "strip-ansi": "^7.1.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/diff": "^6.0.0",
    "@types/node": "^22.9.0",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^2.1.9",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.5"
  }
}
